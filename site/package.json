    "faucet-pipeline-sass": "^1.7.1",
    "faucet-pipeline-static": "^1.2.0",
    "http-proxy-middleware": "^1.1.0",
    "marked": "^2.0.3",
    "npm": "^7.21.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap-icons": "^1.8.3",
    "lightbox2": "^2.11.3"
  }
